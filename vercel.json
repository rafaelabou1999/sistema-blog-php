{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    },
    {
      "src": "public/css/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "public/assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "public/index.php" }
  ]
}